{
    "environments": [

        {
            "name": "los",
            "API_KEY": "DD_LOS_API_KEY",
            "APP_KEY": "DD_LOS_APP_KEY",
            "queries": {
                "los_504" : "env:prod status:error @http.status_code:504",
                "los_502" : "env:prod status:error @http.status_code:502"
            }
        },
        {
            "name": "sba-ulp",
            "API_KEY": "DD_SBA_API_KEY",
            "APP_KEY": "DD_SBA_APP_KEY",
            "queries": {
                "ulp_504" : "env:ulp-prod status:error @http.status_code:504",
                "ulp_502" : "env:ulp-prod status:error @http.status_code:502",
                "ulp_oom" : "env:ulp-prod status:error (OutOfMemoryError OR \"out of memory\" OR OOM)",
                "cls_504" : "env:cls-prod status:error @http.status_code:504",
                "cls_502" : "env:cls-prod status:error @http.status_code:502"
            }
        }
    ]
}